int main() { 
Circle C1; 
Circle C2;
C1.SetRadius(1); 
C2.SetRadius(10); 
cout<<"The area of C1 is "<<C1.AreaOf()<<endl; 
cout<<"The area of C2 is "<<C2.AreaOf()<<endl;
C1.SetRadius(2); 
C2.SetRadius(3);
cout<<"The area of C1 is "<<C1.AreaOf()<<endl; 
cout<<"The area of C2 is "<<C2.AreaOf()<<endl; 
return 0;
}
